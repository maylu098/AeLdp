programa {
  funcao inicio() {
    //Declaração de variáveis
    real altura,peso_ideal

    //Declaração de constantes
    const real k=72.7
    const inteiro b=58

    //Entrada de dados
    escreva("informe a altura")
    leia(altura)

    //Processamento
    peso_ideal=k*altura-b

    //Saída de dados
    escreva ("o seu peso ideal é,peso_ideal")

    
  }
}
